{
  "name": "flutter-print",
  "displayName": "Flutter Print",
  "description": "Help insert and remove print(*) statement",
  "version": "1.1.0",
  "publisher": "ricardo-emerson",
  "repository": "https://github.com/ricardoemerson/flutter-print",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.insertPrintStatement",
    "onCommand:extension.insertPrintWithCurlyBracesStatement",
    "onCommand:extension.deleteAllPrintStatements"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertPrintStatement",
        "title": "Insert Flutter Print Statement"
      },
      {
        "command": "extension.deleteAllPrintStatements",
        "title": "Delete all Flutter Print statements"
      }
    ],
    "keybindings": [
      {
        "command": "extension.insertPrintStatement",
        "key": "shift+ctrl+l",
        "mac": "shift+ctrl+l",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.insertPrintWithCurlyBracesStatement",
        "key": "shift+meta+l",
        "mac": "shift+cmd+l",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.deleteAllPrintStatements",
        "key": "shift+meta+d",
        "mac": "shift+cmd+d",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
